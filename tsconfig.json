{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "es2015"
    ],
    "declaration": true,
    "rootDir": ".",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "moduleResolution": "node16",
    "esModuleInterop": true,
    "importHelpers": true,
    "stripInternal": true
  },
  "exclude": [
    "node_modules",
    "esm",
    "test/data"
  ]
}
